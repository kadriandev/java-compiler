/* A program to perform Euclid's 
   algorithm to compute a gcd */

int z[2];

int gcd(int u, int v) {
  if (v == 0) return u;
  else return gcd(v, u - u/v*v);
}

void main(void) {
  int x[3]; int y[3]; int z;
  x = z;
  z = x;
  y = x;
  x[1] = 3;
  z = x[1];
  y = input();
  output(gcd(x[1], y));
}
